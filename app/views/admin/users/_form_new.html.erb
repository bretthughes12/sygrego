<%= simple_form_for(@user, url: url, html: { method: method }) do |f| %> 
	<%= f.error_notification %>

	<%= f.input :email %>
	<%= f.input :password %>
	<%= f.input :password_confirmation %>
	<%= f.input :protect_password %>

	<%= f.input :name %>
	<%= f.input :status, as: :select, collection: User::STATUS, include_blank: false %>
	<%= f.input :address %>
	<%= f.input :suburb %>
	<%= f.input :postcode %>
	<%= f.input :phone_number %>
	<%= f.input :wwcc_number %>
	<%= f.input :group_role %>
	<%= f.input :years_as_gc %>

	<%= f.button :submit %>
<% end %>	

id,section_id,match,draw_number,complete,court,entry_a_id,score_a,forfeit_a,entry_b_id,score_b,forfeit_b,entry_umpire_id,forfeit_umpire
<%= CSV.generate do |csv|
  @round_robin_matches.each do |r|
	csv << [ 
		r.id,
		r.section_id, 
		r.match,
		r.draw_number,
		r.complete,
		r.court,
		r.entry_a_id,
		r.score_a,
		r.forfeit_a,
		r.entry_b_id,
		r.score_b,
		r.forfeit_b,
		r.entry_umpire_id,
		r.forfeit_umpire
	] 
  end
end.html_safe %>

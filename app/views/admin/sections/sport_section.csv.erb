<%- headings = %w{RowID Grade Name Active Venue YearIntroduced NbrOfCourts Session FinalsFormat Groups StartCourt } -%>
<%= headings.join(',') %>
<%= CSV.generate do |csv|
  @sections.each do |section|
  	line = [ section.database_rowid,
  		     section.grade.name,
  		     section.name,
  		     section.active,
  		     section.venue.database_code,
  		     section.year_introduced,
  		     section.number_of_courts,
			 section.session.database_rowid,
			 section.finals_format,
			 section.number_of_groups,
			 section.start_court
			]
    csv << line
  end
end.html_safe %>
